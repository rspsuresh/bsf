<script>
var oTable = $("#sequentialView").dataTable({"aLengthMenu": [[1, 25, 50, -1], [1, 25, 50, "All"]],
"initComplete": function(settings, json) {
$("select[name=sequentialView_length]").selectpicker();
},	
"drawCallback": function( settings ){
$(this).find("tr.mainTr").trigger('click');
}

});

function trClick(){
oTable.$("tr.mainTr").unbind();
oTable.$("tr.mainTr").bind('click', function(){
if(!($(this).next("tr").hasClass("duplicateTr"))){
$(this).after("<tr class='duplicateTr'>"+$(".subTr_"+$(this).attr("id")).html()+"</tr>");
$(this).next(".subTr_"+$(this).attr("id")).show();
$(this).next("tr").find('.subDiv').slideDown("slow");
changePage();
}
});
}	
trClick();
oTable.$("tr.mainTr").trigger("click");
function rfqDelete( cnt = "<?php echo count($RfqUsedVal); ?>")
{
	if(cnt>0){
		txt ="We have received Some responses Form vendor. \n Do you still wanna Delete the Rfq ?";
	} else {
		txt ="Do you want to delete this RFQ?";
	}
    var val =confirm(txt);
    if(val == true)
    {
        $.ajax({
            url:getBaseURL()+'ats/rfq/delete',
            type:'post',
            data:{"RfqId":<?php echo $RfqId; ?>},
            async:false,

            success:function(data, textStatus, jqXHR){
                if (jqXHR.status != 200) {
                    alert('Failed to delete the RFQ Entry !');
                    return false;
                }
                else
                {
                    alert('Deleted Successfully');
                    window.location.href =  getBaseURL() + 'ats/rfq/rfq-register';
                }
            },
            error:function(jqXHR, textStatus, errorThrown){
                alert('Failed to delete the RFQ Entry !');
            }
        });

    }
}
</script>
<!--table header script end-->
