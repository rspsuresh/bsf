<script type="text/javascript">
    function LoadVendorCatg(vendId) {
        $('#largeker').modal('show');
        var typeId=3;
        var vendId=vendId;

        $.ajax({
            url:getBaseURL()+"users/index/finalsummary",
            type:"post",
            data:{'typeId':typeId,"VendorId": vendId},
            async: false,
            //dataType:"json",
            success:function(data, textStatus, jqXHR){
                //console.log(data);
                $('#enquiryWrapper1').html('');
                $('#enquiryWrapper1').html(data);
            },
            error:function(jqXHR, textStatus, errorThrown){
            }
        });
    }

    function Updateapprovedetail(rfqId) {
        var typeId=1;
        var rfqId=rfqId;

        $.ajax({
            url:getBaseURL()+"users/index/finalsummary",
            type:"post",
            data:{'typeId':typeId,"RFQId": rfqId},
            async: false,
            //dataType:"json",
            success:function(data, textStatus, jqXHR) {
                //console.log(data);
                if (data == "Approve RFQ Entry Successfully") {
                    $('#largeker1').modal('show');
                    $('#enquiryWrapper2').html('');
                    $('#enquiryWrapper2').html(data);
                    setTimeout(function(){
                        $('#largeker1').modal('hide');
                        window.location.href = getBaseURL() + "users/index/billingpaymentconfirmation/" + rfqId;
                    }, 3000);

                } else if (data == "Email sent to Superior for Approval") {
                    $('#largeker1').modal('show');
                    $('#enquiryWrapper2').html('');
                    $('#enquiryWrapper2').html(data);
                    setTimeout(function(){
                        $('#largeker1').modal('hide');
                        window.location.href = getBaseURL() + "users/index/rfqapprovedetail/2/" + rfqId;
                    }, 3000);

                } else {
                    alert(data);
                }
            },
            error:function(jqXHR, textStatus, errorThrown){
            }
        });
    }

    function LoadTermsHistory(rfqId, vendId) {
        $('#largequicker1').modal('show');
        var typeId=2;
        var vendId=vendId;
        var rfqId=rfqId;

        $.ajax({
            url:getBaseURL()+"users/index/finalsummary",
            type:"post",
            data:{'typeId':typeId,"VendorId": vendId,"RFQId": rfqId},
            async: false,
            //dataType:"json",
            success:function(data, textStatus, jqXHR){
                //console.log(data);
                $('#enquiryWrapper').html('');
                $('#enquiryWrapper').html(data);
            },
            error:function(jqXHR, textStatus, errorThrown){
            }
        });
    }
</script>