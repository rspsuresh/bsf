                            dAmt = dNetAmt;
                        } else if (iQualTypeid==2) {
                            var dTaxablePer = parseFloatVal(isNullCheck($('#Qual_' + key1 + '_TaxablePer_' + i).val(), 'number'));
                            var dTaxPer = parseFloatVal(isNullCheck($('#Qual_' + key1 + '_TaxPer_' + i).val(), 'number'));
