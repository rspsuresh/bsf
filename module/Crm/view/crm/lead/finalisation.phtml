                            $('#Qual_' + key1 + '_NetPer_' + i).val(dNetPer);

                            $('#Qual_' + key1 + '_NetPer_' + i).val(sanitizeNumber(dNetPer, 2));
                            $('#Qual_' + key1 + '_ExpPer_' + i).val(sanitizeNumber(dNetPer, 2));

