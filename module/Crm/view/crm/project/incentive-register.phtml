addRow();
function addRow(){
	$( ".newRow" ).unbind();
	$( ".newRow" ).bind('keyup', function() {
	//alert($(this).closest("tr").index()+"------------"+$("#contactTable tbody tr:last").index())
		if($(this).val().length>0 && $(this).closest("tr").index() == $("#contactTable tbody tr:last").index()){
			$("#contactTable tbody").append("<tr>"+$("#contactTable tbody tr:first").html()+"</tr>");
				$("#contactTable tbody tr").each(function(){
				var index=$(this).index() + 1;
				var lastIndex=$("#contactTable tbody tr:last").index() + 1;
				$(this).find("select, input:text").each(function(){
					if(index==lastIndex){
						if($(this).prop("tagName").toLowerCase() == 'input')
							$(this).val("");
						else
							$(this).val("1");
					}
					$(this).attr("name", $(this).attr("tagname")+'_'+index);
					$(this).attr("id", $(this).attr("tagname")+'_'+index);		
				})
			});	
			removetr();
			addRow();
		}
		if($(this).val().length>0 && $(this).closest("tr").index() == $("#percentTable tbody tr:last").index()){
			$("#percentTable tbody").append("<tr>"+$("#percentTable tbody tr:first").html()+"</tr>");
				$("#percentTable tbody tr").each(function(){
				var index=$(this).index() + 1;
				var lastIndex=$("#percentTable tbody tr:last").index() + 1;
				$(this).find("select, input:text").each(function(){
					if(index==lastIndex){
						if($(this).prop("tagName").toLowerCase() == 'input')
							$(this).val("");
						else
							$(this).val("1");
					}
					$(this).attr("name", $(this).attr("tagname")+'_'+index);
					$(this).attr("id", $(this).attr("tagname")+'_'+index);		
				})
			});	
			removetr();
			addRow();
		}
	});
}

function slabCalAmt(thsInp, inpId)
{
    if(thsInp.value != '') {
        var thsId = thsInp.id;
        var spthisId = thsId.split('_');
        if(parseInt(spthisId[1])!=1) {
            var prevInp = (parseInt(spthisId[1]) - parseInt(1));
            var prevVal = (parseInt(thsInp.value) - parseInt(1));
            var oldVal=0;
            if(!isNaN(parseInt($('#' + spthisId[0] + '_' + prevInp).val()))) {
                oldVal=parseInt($('#' + spthisId[0] + '_' + prevInp).val());
            }
            var newVal = 0;
            if(!isNaN(parseInt(thsInp.value))) {
                newVal=parseInt(thsInp.value);
            }
            if ( newVal> oldVal) {
                $('#' + inpId + '_' + prevInp).val(prevVal);
            } else {
                $('#' + thsId).val('');
                $('#' + thsId).focus();
                return false;
            }
        }
    }
}

