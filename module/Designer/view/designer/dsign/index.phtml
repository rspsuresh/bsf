.info-tooltip {
  display: inline;
  background-color: #4183D7;
  border-radius: 100%;
  position: relative;
  color: #fff;
  vertical-align: baseline;
  font-size: 80%;
  padding: 4px 10px;
  border: 2px solid #4183D7;
  line-height: 1;
  position: relative;
}
.info-tooltip:before {
  position: absolute;
  background-color: #fff;
  border: 2px solid #4183D7;
  padding: 5px 10px;
  min-width: 180px;
  max-width: 200px;
  height:40px;
  bottom: 70%;
  left: 125%;
  transform: translateY(60%);
  content: attr(data-content);
  color: #f00;
  display: none;
  font-size: 13px;
  font-weight:600;
  border-radius: 2px;
  z-index: 100;
  text-align: center;
  line-height: 2;
  -webkit-animation: swooshUp 200ms cubic-bezier(0.25, 0.5, 0.5, 0.9);
  animation: swooshUp 200ms cubic-bezier(0.25, 0.5, 0.5, 0.9);
}
.info-tooltip:after {
  position: absolute;
  right: auto;
  bottom: 70%;
  left: 40%;
  transform: translateY(60%);
  border-width: 12px 15px 0px 15px;
  border-color: transparent #4183D7 transparent transparent ;
  content: " ";
  display: none;
  margin: 0;
  border-style: solid;
  z-index: 3;
  -webkit-animation: swooshUp 200ms cubic-bezier(0.25, 0.5, 0.5, 0.9);
  /*animation: swooshUp 200ms cubic-bezier(0.25, 0.5, 0.5, 0.9);*/
}
.info-tooltip:hover, .info-tooltip:focus {
  background-color: #fff;
}
.info-tooltip:hover:before, .info-tooltip:hover:after, .info-tooltip:focus:before, .info-tooltip:focus:after {
  display: block;
}

