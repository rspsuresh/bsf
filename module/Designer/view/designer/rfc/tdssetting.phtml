<script>
$(function(){
    $('.rightbox_trigger').click(function(){ 
		$('.block_panel').addClass('block_panel_open');
		$('.right_lightbox').show();
		$('.left-panel').removeClass('collapsed');
		$('.l_menuicon_nav').removeClass('cross');
		$(this).parent().find('.slot_no').css({'background-color':'#636363'});
		$(this).parent().find('.slot_no p').css({'color':'#fff'});
    });
	$('.right_lightbox_close').click(function(){
		$('.right_lightbox').hide();
		$('.block_panel').removeClass('block_panel_open');
		if ($(window).width() >=980){
			$('.left-panel').addClass('collapsed');	
			$('.l_menuicon_nav').addClass('cross');
		}
	});
});
$(".mainTr").click(function(e){0
	e.preventDefault();
	if(!$(this).closest("tr").next(".subTr").is(":visible")){
		$(this).closest("tr").next(".subTr").show();
		$(this).closest("tr").next(".subTr").find(".subDiv").slideDown("slow");	
		$(this).find("i").addClass("tform");
	}
		else{
		$(this).closest("tr").next(".subTr").find(".subDiv").slideUp("slow");
		$(this).closest("tr").next(".subTr").slideUp("slow");
		$(this).find("i").removeClass("tform");
	}
})

</script> 
