        <div class="row">
            <div class="col-lg-12">
            	<h1>Rfq-Register</h1> 
            </div>
            <div class="col-lg-12 clear">
                <div class="table-responsive clear">
                    <div id="jqxWidget">
                    <div id="jqxgrid"></div>
                </div>
                </div>
            </div>
        </div>
    </div>
</div>
