			var str = '000000';
			var splitVal = val1.split('.');
			var len = 6;
			if(splitVal[1]){
				len -= splitVal[1].length;
			}
			if(len == 6)
				val = val1+'.000000';
			else if(len < 0)
				val = splitVal[0]+'.'+splitVal[1].substring(0,6);
			else
