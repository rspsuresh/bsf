    else if ($(this).val() == 'underline'){
        if ($(this).is(':checked')) $('input[name="Textbold"]').css('text-decoration', 'underline');
        else $('input[name="Textbold"]').css('text-decoration', 'none');
    }
});
$(window).scroll(function(){
  var sticky = $('.sticky'),
      scroll = $(window).scrollTop();

  if (scroll >= 100) sticky.addClass('topfixingheade');
  else sticky.removeClass('topfixingheade');
});

</script> 

