$(".special").keydown(function (e) {
    var key = e.which || e.keyCode;
    if (!e.shiftKey && !e.altKey && !e.ctrlKey &&
            // numbers
        key >= 48 && key <= 57 ||
            // Numeric keypad
        key == 46 || key == 45){
        return false;
    }
    else{
        return true;
    }

});

