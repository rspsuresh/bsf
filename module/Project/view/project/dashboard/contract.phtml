<script type="text/javascript">
    // render remainders
    var arr_reminders = <?php echo (isset($reminders)) ? json_encode($reminders) : '[]';?>;
    var template = $('#remainder-template').html();
    var vertial_ticker = $('#vertical-ticker');
    if(arr_reminders.length != 0) {
        $.each(arr_reminders, function (i, o) {
            var message = "Checklist '" + o.CheckListName+ "'";
            if(o.TodayDue == '1') {
                message += " has due today!"
            } else {
                message += " is overdue!"
            }
            vertial_ticker.append(template.replace(/\{\{username\}\}/g, o.UserName)
                .replace(/\{\{img_url\}\}/g, (o.UserLogo == '') ? '/images/pro_user.png' : o.UserLogo )
                .replace(/\{\{datetime\}\}/g, o.TargetDate)
                .replace(/\{\{message\}\}/g, message));
        });
    }
</script>
