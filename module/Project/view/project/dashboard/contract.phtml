<script>
    var competitorsData = <?php echo (isset($bidcompetitors)) ? json_encode($bidcompetitors) : '[]'; ?>;
    $(function () {
        bindCompetitorStandChart(competitorsData);
    });

    function bindCompetitorStandChart(competitorsData) {
        var chartCategories = [];
        var chartSeries = [];
        if(competitorsData.length != 0 ) {
            $.each(competitorsData, function (i,o) {
                if ($.inArray(o.Position, chartCategories) == -1) {
                    chartCategories.push(o.Position);
                }
            });

            $.each(competitorsData, function (i,o) {
                var index = $.inArray(o.Position, chartCategories);
                // check compname
                var isFound = false;
                $.each(chartSeries, function (j,obj) {
                    if(obj.name == o.CompetitorName) {
                        isFound = true;
                        obj.data[index] = parseInt(o.count);
                        return false;
                    }
                });

                if(!isFound) {
                    var seriesObj = {
                        type: 'column',
                        name: o.CompetitorName,
                        data: new Array(chartCategories.length).fill(0)
                    };
                    seriesObj.data[index] = parseInt(o.count);
                    chartSeries.push(seriesObj);
                }
            });
        }

        $('#land-chart10').highcharts({
            title: {
                text: 'Combination chart'
            },
            xAxis: {
                categories: chartCategories
            },
            series: chartSeries
        });
    }
</script>
