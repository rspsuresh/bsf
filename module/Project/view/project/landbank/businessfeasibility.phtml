    function setSalableArea(x) {
        var $x = $(x);
        var Salid = x.id;
        if(Salid == 'PropSaleableArea') {
            $('#SaleableArea').val($x.val());
        } else if(Salid == 'SaleableArea') {
            $('#PropSaleableArea').val($x.val());
        } else if(Salid == 'SaleableAreaUnitId') {
            $('#PropSaleableAreaUnitId').val($x.val());
            $('#PropSaleableAreaUnitId').selectpicker('refresh');
            var Selectedtxt = $("#SaleableAreaUnitId option:selected").text();
            $('#LeasableAreaUnit').val(Selectedtxt);
            $('#CommonAreaUnit').val(Selectedtxt);
            $('#TotalAreaUnit').val(Selectedtxt);
        } else if(Salid == 'PropSaleableAreaUnitId') {
            $('#SaleableAreaUnitId').val($x.val());
            $('#SaleableAreaUnitId').selectpicker('refresh');
        }
        //Calculate Total Area
        var SaleableArea = parseFloat(isNullCheck($('#SaleableArea').val(),'number'));
        var LeasableArea = parseFloat(isNullCheck($('#LeasableArea').val(),'number'));
        var CommonArea = parseFloat(isNullCheck($('#CommonArea').val(),'number'));
        var TotalArea = $('#TotalArea');
        var Total = parseFloat(SaleableArea) + parseFloat(LeasableArea) + parseFloat(CommonArea);
        TotalArea.val(Total);

    }

    function calcTotal() {
