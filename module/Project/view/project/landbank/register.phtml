function bindMouseover() {
    $(".sortoption").change(function() {
        registerreload();
    });
    $(".circle").mouseover(function() {
        $this = $(this);
        $parent = $this.parent();
        $parent.find('.label').show();
    });
    $(".label").mouseover(function() {
        $this = $(this);
        $parent = $this.parent();
        $parent.find('.label').show();
    });
    $(".circle").mouseout(function() {
        $this = $(this);
        $parent = $this.parent();
        $parent.find('.label').hide();
    });
    $(".label").mouseout(function() {
        $this = $(this);
        $parent = $this.parent();
        $parent.find('.label').hide();
    });
}

