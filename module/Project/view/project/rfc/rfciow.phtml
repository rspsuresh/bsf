    var sRowType = $.trim(isNullCheck($('#'+id).val(),'string'));
    if (sRowType =="R" || sRowType =="I" || sRowType =="H" || sRowType =="WG" ) removeError($('#' + id));
    else showError($('#' + id), 'Enter RowType');
