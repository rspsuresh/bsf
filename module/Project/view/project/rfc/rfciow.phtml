        if (arrSubIOWAnal[i].resourceid == resid) {
            arrSubIOWAnal[i].rate = rate;
            arrSubIOWAnal[i].amount = parseFloat(arrSubIOWAnal[i].qty) * rate;
            UpdateSubIOWRate(arrSubIOWAnal[i].subiowid);
