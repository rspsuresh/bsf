    function excelvalid() {
        var $excelpids = $('input[id^=excelworktype_]');
        $.each($excelpids, function (i, obj) {
            var $this = $(this),
                name = $this[0].id;
            var ikey = name.split('_')[1];
            if (isNullCheck($("#excelworktypeid_" + ikey).val(), 'number') == 0)  $('#excelworktype_' + ikey).addClass('error');
            else  $("#excelworktype_" + ikey).prop('readonly', true);
            var sgName = isNullCheck($("#excelworkgroup_" + ikey).val().trim(),'string');
            if (sgName != "") {
                if (checkWorkgroupNameUsed(sgName, ikey) == false) $('#excelworkgroup_' + ikey).addClass('error');
                else  $("#excelworkgroup_" + ikey).prop('readonly', true);
            } else {
                $('#excelworkgroup_' + ikey).addClass('error');
            }
        });
    }
    function checkWorkgroupNameUsed(sgName,irowid) {
        var isValid = true;
        $.ajax({
            url: getBaseURL() + "project/rfc/checkworkgroupnamefound/"+sgName,
            async: false,
            type: 'post',
            success: function(data,status) {
                if (data == "Y") {
                    isValid =false;
                }
            }
        });
        if (isValid==false) return isValid;
        var $codes = $('input[id^=workgroupname_]');
        $.each($codes, function (i, obj) {
            var $this = $(this),
                name = $this[0].id;
            if (sgName == $('#' + name).val().trim()) {
                isValid=false;
                return;
            }
        });
        if (isValid==false) return isValid;
        var $excelcodes = $('input[id^=excelworkgroup_]');
        $.each($excelcodes, function (i, obj) {
            var $this = $(this),
                name = $this[0].id;
            var ickey = isNullCheck(name.split('_')[1],'number');
            if (ickey !=0) {
                if (ickey != irowid) {
                    if (sgName == $('#excelworkgroup_' + ickey).val().trim()) {
                        isValid = false;
                        return;
                    }
                }
            }
        });
    }
