    function checkWorkGroupName(x) {
        var iARowid = x.split("_")[1],
            $workgroupname =  $('#workgroupname_' + iARowid),
            sGroupName =  $workgroupname.val().trim(),
            iWorkGroupId =  $('#workgroupid_' + iARowid).val(),
            irfcid = $('#rfcUId').val();

        removeError($workgroupname);
        if (sGroupName.length ==0) { return; }
//    var sMode ="add";
//    if (isNullCheck($('#rfcmode').val(),'string') == 'edit') smode='edit';
        $.ajax({
            url: getBaseURL() + "project/rfc/checkworkgroupnamefound",
            async: false,
            type: 'post',
            data: {'groupid': iWorkGroupId, 'groupname':sGroupName,'rfcid':irfcid},
            success: function(data,status) {
                if (data == "Y") {
                    showError($workgroupname, 'Already used');
                    return;
                }
            }
        });
        if ($workgroupname.hasClass('error')) return;
        removeError($workgroupname);

        var $codes = $('input[id^=workgroupname_]');
        $.each($codes, function (i, obj) {
            var $this = $(this),
                name = $this[0].id;
            var ikey = isNullCheck(name.split('_')[1],'number');
            if (ikey !=0) {
                if (ikey != iARowid) {
                    if (sGroupName == $('#'+name).val().trim()) {
                        showError($workgroupname, 'Already used');
                        return;
                    }
                }
            }
        });
    }

