        if($('#vehicleId').val() == "" || $('#vehicleId').val() == null){
            $('#vehicleId').closest('.form-group').find(".error_message").show();
            errorFlagCount = errorFlagCount + 1;
            errorFlag = 1;
            if(errorFlagCount == 1) {
                $('#vehicleId').focus();
            }
        }
        if(errorFlag == 0) {
            return true;
        } else {
            return false;
        }
    }

