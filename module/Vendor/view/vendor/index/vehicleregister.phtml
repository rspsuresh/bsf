function addRow(){
    $( ".newRow" ).unbind();
    $( ".newRow" ).bind('keyup', function() {
    //alert($(this).closest("tr").index()+"------------"+$("#contactTable tbody tr:last").index())
        if($(this).val().length>0 && $(this).closest("tr").index() == $("#contactTable tbody tr:last").index()){
            $("#contactTable tbody").append("<tr>"+$("#contactTable tbody tr:first").html()+"</tr>");
                $("#contactTable tbody tr").each(function(){
                var index=$(this).index() + 1;
                var lastIndex=$("#contactTable tbody tr:last").index() + 1;
                $(this).find("select, input:text").each(function(){
                    if(index==lastIndex){
                        if($(this).prop("tagName").toLowerCase() == 'input')
                            $(this).val("");
                        else
                            $(this).val("1");
                    }
                    $(this).attr("name", $(this).attr("tagname")+'_'+index);
                    $(this).attr("id", $(this).attr("tagname")+'_'+index);       
                })
            });   
            // removetr();
            addRow();
        }
    });
}

function deleteRow(x,e,data) {
	var tId = data;
    e.preventDefault();
	 if (!confirm('Do you want to Delete'))
        return false;
    var $x = $(x), 
        key = $x[0].className.split('_')[1]; 
		var tId= $('#vehicleId_'+ key).val(); 
		if(tId!=""){			
			$.ajax({
				type: 'post',
				url: getBaseURL()+'vendor/index/vehicleregister',
				data: "VehicleId="+tId+"&Type=delete",
				success: function (data) {
					$('#tr_'+key).remove();
					window.location.replace(getBaseURL() + "vendor/index/vehicleregister/<?php echo $this->bsf->encode($vendorId); ?>");
				}
			});
		}else{
			$('#tr_'+key).remove();
		}	
}
