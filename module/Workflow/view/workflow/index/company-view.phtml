    $('.about').hide();
    $('.contacts').show();
    $('.statutory').hide();
    $('.branch').hide();
});
function newInfo(ele) {
    var inform = $(ele)[0].id;
    $('.'+inform).show();
    $('#'+inform).addClass('current');
    switch (inform) {
        case 'about':
            $('.head').html('About');
            $('.timeline').hide();
            $('.contacts').hide();
            $('.statutory').hide();
            $('.branch').hide();
            $('#timeline').removeClass('current');
            $('#contacts').removeClass('current');
            $('#statutory').removeClass('current');
            $('#branch').removeClass('current');
            break;
        case 'timeline':
            $('.head').html('Time Line');
            $('.about').hide();
            $('.contacts').hide();
            $('.statutory').hide();
            $('.branch').hide();
            $('#about').removeClass('current');
            $('#contacts').removeClass('current');
            $('#statutory').removeClass('current');
            $('#branch').removeClass('current');
            break;
        case 'contacts':
            $('.head').html('Contacts');
            $('.timeline').hide();
            $('.about').hide();
            $('.statutory').hide();
            $('.branch').hide();
            $('#timeline').removeClass('current');
            $('#about').removeClass('current');
            $('#statutory').removeClass('current');
            $('#branch').removeClass('current');
            break;
        case 'statutory':
            $('.head').html('Statutory Info');
            $('.timeline').hide();
            $('.contacts').hide();
            $('.about').hide();
            $('.branch').hide();
            $('#timeline').removeClass('current');
            $('#contacts').removeClass('current');
            $('#about').removeClass('current');
            $('#branch').removeClass('current');
            break;
        case 'branch':
            $('.head').html('Branch Info');
            $('.timeline').hide();
            $('.contacts').hide();
            $('.statutory').hide();
            $('.about').hide();
            $('#timeline').removeClass('current');
            $('#contacts').removeClass('current');
            $('#statutory').removeClass('current');
            $('#about').removeClass('current');
            break;
        default:
            $('.head').html('Time Line');
            $('.timeline').show();
            $('.about').hide();
            $('.contacts').hide();
            $('.statutory').hide();
            $('.branch').hide();
            $('#timeline').addClass('current');
            $('#about').removeClass('current');
            $('#contacts').removeClass('current');
            $('#statutory').removeClass('current');
            $('#branch').removeClass('current');
            break;
    }

}
</script>