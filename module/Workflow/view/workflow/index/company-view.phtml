}, 900, 'swing', function () {
	if (windscroll > 80)
	window.location.hash = target;
	});
	});

	$("#back-top").fadeOut();
	$(window).scroll(function(){
		if ($(this).scrollTop() > 100) {
			$("#back-top").fadeIn();
		} else {
			$("#back-top").fadeOut();
		}
	}); 
