    function addNewRow(x) {
        var strClass = $(x).attr('id');
        var arr = strClass.split('_');
        var newCount = parseInt($('#'+arr[0]+'_count').val());
        var template = $('#'+arr[0]+'dummy-row').html();
        newCount++;
        template = template.replace(/__/g, '_' + newCount);
        $('.full_'+arr[0]).append(template);
        $('#'+arr[0]+'_count').val(newCount);
    }

function validatePhone(evt){
    var charCode = (evt.which) ? evt.which : evt.keyCode;
    if (charCode > 31 && (charCode < 48 || charCode > 57) && charCode !=37 && charCode!=39 && charCode!=46 && charCode!=45)
        return false;
    return true;
}
function showErrorMsg($tarIp, msg) {
    var $tarErrorMsg = $tarIp.parent().next('.error_message');
    $tarErrorMsg.find('p').text(msg);
    $tarErrorMsg.show();
}

function removeErrorMsg($tarIp) {
    var $tarErrorMsg = $tarIp.parent().next('.error_message');
    $tarErrorMsg.hide();
}
function isNumberKey(evt){
    var charCode = (evt.which) ? evt.which : evt.keyCode;
    if(charCode > 31 && (charCode < 48 || charCode > 57) && charCode!=39)
        return false;
    return true;
}

