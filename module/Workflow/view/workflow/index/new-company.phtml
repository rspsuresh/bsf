function validateStatutory() {
    $PANno = $("#pan_no");
    $TANno = $("#tan_no");

    var errorFlag = 0;
    var errorFlagCount = 0;

    if($PANno.val() == "" || !validatePanNumber($PANno.val())) {
        $PANno.addClass('red_bdr');
        showErrorMsg($PANno, "Invalid PAN No");
        errorFlagCount = errorFlagCount + 1;
        errorFlag = 1;
        if(errorFlagCount == 1) {
            $PANno.focus();
        }
    } else {
        removeErrorMsg($PANno);
        $PANno.removeClass('red_bdr');
    }

    if($TANno.val() == "" || !validateTanNumber($TANno.val())) {
        showErrorMsg($TANno, "Invalid TAN No");
        $TANno.addClass('red_bdr');
        errorFlagCount = errorFlagCount + 1;
        errorFlag = 1;
        if(errorFlagCount == 1) {
            $TANno.focus();
        }
    } else {
        removeErrorMsg($TANno);
        $TANno.removeClass('red_bdr');
    }

    if(errorFlag == 0) {
        $("#step4").addClass('opacity_08');
        $("#step4").children("div").removeClass('stepsep_active');
        $("#step5").removeClass('opacity_08');
        $("#step5").children("div").addClass('stepsep_active');
        if ($('#companyid').val() ==0) $('#panelheading-3').show();
        $('#panelheading-3').trigger('click');
//        $(".statutory").hide();
//        $(".branch").show();
    }
}

