$('#btnDeleteYes').on('click', function () {
    var remarks = $remarks.val();
    //console.log($('#delDeptName').val())
    if(remarks.length == 0) {
        showError($remarks, 'Required!');
        return;
    } else {
        removeError($remarks);
    }

    $.ajax({
        url: getBaseURL() + 'workflow/index/deleteorgdepartment',
        type: 'POST',
        data: {"DepId": $('#deleteDeptId').val(), "Remarks": remarks, "Type": 'update'},
        async: false,
        success:function(data, textStatus, jqXHR){
            if (jqXHR.status == 200) {
                $('#index_'+$deleteModal.attr('data-rowid')).remove();
                $('#rtDivDept_'+$deleteModal.attr('data-rowid')).remove();
            } else
                alert('Cannot delete this Department, it is already in use!');
        },
        error:function(jqXHR, textStatus, errorThrown){
            alert('Failed to delete the Department!');
        }
    });
    $deleteModal.modal('hide');
    $remarks.val('');
});

