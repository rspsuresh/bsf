    $('#newpositionname').val('');
    $('#AddPositionModal').modal('show');
}

function departmentSearch(positionDetails) {
    if (globalTimeout != null) {
        clearTimeout(globalTimeout);
    }
    globalTimeout = setTimeout(function() {

        $.ajax({
            url: getBaseURL() + 'workflow/index/organisationdepartment',
            type: 'POST',
            data: {'uMode': 'searchpos','searchVal' : $.trim(positionDetails)},
            async:false,
            success: function (data, textStatus, jqXHR) {
                var posDetail = JSON.parse(data);
                $('.table tbody').html('');
                var sHtml="";
                if(posDetail.length>0) {
                    $.each(posDetail,function(j,o) {
                        var i = +j+1;
                        sHtml = sHtml + '<tr id="index_' + i + '">'+
                        '<td>' + i + '</td>' +
                        '<td>' + o.Dept_Name + '</td> ' +
                        '<td>' + o.Dept_Type + '</td> ' +
                        '<td class="action_btns_td">' +
                        '<ul class="action_btns">'+
                        '<li>'+
                        '<a href="javascript:void(0);" data-toggle="tooltip" data-placement="left" data-original-title="Delete" onclick="clientDelete(this,' + o.DeptId + ',' + o.Dept_Name + ',' + i + ');">' +
                        '<span><i class="fa fa-trash-o"></i></span>' +
                        '</a>'+
                        '</li>'+
                        '<li>'+
                        '<a href="javascript:void(0);" data-toggle="tooltip" class="mainTr" data-expandid="' + o.DeptId + '" onclick="editDept(' + o.DeptId + ',$(this));getDeptId(this);" rel="rt" data-placement="left" data-original-title="Edit">'+
                        '<span><i class="fa fa-edit"></i></span>'+
                        '</a></li></ul></td></tr>';
                    });
                }
                $('.table tbody').html(sHtml);
            },
            error: function (jqXHR, textStatus, errorThrown) {
                if (jqXHR.status == 400)
                    alert(jqXHR.responseText);
                else
                    alert("Request Failed");

            }
        });
        globalTimeout=null;
    }, 500);
}

function checkPositionFound(argPosition) {
    removeError($('#newpositionname'));
    $.ajax({
        url:getBaseURL()+ 'workflow/index/checkpositionFound',
        type:"post",
        data: {'positionId':0,'positionName':argPosition},
        async: false,
        success:function(data, textStatus, jqXHR){
            if(data == 'Y'){
                showError($('#newpositionname'), 'Position Name already found!');
                return false;
            } else{
                removeError($('#newpositionname'));
            }
        },
        error:function(jqXHR, textStatus, errorThrown){
            return false;
        }
    });
}

function newPositionUpdate(argPosition) {
    arrPositionMaster = [];
    $.ajax({
        url:getBaseURL()+ 'workflow/index/addnewposition',
        type:"post",
        data: {'positionId':0,'positionName':argPosition},
        async: false,
        success:function(data, textStatus, jqXHR){
            arrPositionMaster = JSON.parse(data);
            showPosition();
        },
        error:function(jqXHR, textStatus, errorThrown){
