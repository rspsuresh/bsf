function checkSelectAll(cId) {
    if($('*[data-id='+cId+']:checked').length != $('*[data-id='+cId+']').length) {
        $("#select_all_"+cId).prop('checked',false);
    } else {
        $("#select_all_"+cId).prop('checked',true);
    }
}
function createNewPosition() {
    var sPositionName = isNullCheck($('#newpositionname').val(),'string');
    if (sPositionName=="") { showError($('#newpositionname'), 'Enter PositionName'); return; }
    checkPositionFound(sPositionName);
    if ($('#newpositionname').hasClass('error')) return;
    newPositionUpdate(sPositionName);
    $('#AddPositionModal').modal('hide');
}

