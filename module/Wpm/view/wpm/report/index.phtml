<script type="text/javascript">
    $(function () {
        var $worktype = $('#worktype'),
            $CostCentre = $('#CostCentre');

        $('.dependency_nav').on('click', function (e) {
            var costCentreId = $CostCentre.val();
            if(costCentreId.length != 0) {
                $(this).attr('href', $(this).attr('href') + '/' + costCentreId);
            } else {
                $(this).attr('href', $(this).attr('href').replace(/\/[0-9]+$/g, ''));
            }
        });
    });
</script>