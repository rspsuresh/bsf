<script>
    function Imagebrowse(id) {
        var id = $(id)[0].id;

        var arr = id.split('_')[1];
        console.log(arr);
        console.log( $("#myfile_1"));
        $("#myfile_1").click();
        $('#myfile_1').change(function() {
            readURL(this,arr);
        });
    }
    function readURL(input, value) {
        if (input.files && input.files[0]) {
            var reader = new FileReader();
            //var id = value;
            reader.onload = function(e) {
                $("#showpic_1").hide();
                $("#req_LogoPath").show();
                $('#req_LogoPath').attr('src', e.target.result);
            }

            reader.readAsDataURL(input.files[0]);
        }
    }
    $("#textMe").keypress(function(){
        var tex= $("textarea#textBox").val();
        $("textarea#textBox").text(tex);
    });
</script>

